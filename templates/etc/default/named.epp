# Managed by Puppet

# run resolvconf?
<% if $bind::resolvconf_service_enable { -%>
RESOLVCONF=yes
<% } else { -%>
RESOLVCONF=no
<% } -%>

# startup options for the server
OPTIONS="-u '<%= $bind::service_user %>' -c '<%= $bind::service_config_file %>' <%= $bind::service_options %>"
